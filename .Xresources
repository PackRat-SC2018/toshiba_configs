! Urxvt ----------------------------------------------------------------
URxvt*termName:          rxvt-256color
URxvt*geometry:          92x32
URxvt.buffered:          true
URxvt*scrollBar:         false
URxvt*scrollBar_right:   false
URxvt*jumpScroll:        true
URxvt*skipScroll:        true
URxvt.secondaryScroll:   true
URxvt*scrollstyle:       plain
URxvt*scrollTtyOutput:   true
URxvt*saveLines:         4096
URxvt*internalBorder:    4
URxvt.cutchars:          "'*,<>[]{}|-_@:/()â”‚"

URxvt*transparent:       false
URxvt*depth:             32
URxvt.background:        [96]#121314
URxvt*fading:            55
URxvt*highlightColor:    #D1C384
URxvt*highlightTextColor:  #0D0D0D

!URxvt.font:              -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso8859-1
!URxct.boldFont:          -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso8859-1
!*antialias:              true
!*autohint:               true

URxvt.font:              xft:Fira Code:Regular:size=10:antialias=true:autohint=true
URxvt.boldFont:          xft:Fira Code:Regular:size=10:antialias=true:autohint=true
URxvt.letterSpace: 1

URxvt.perl-ext-common: default,matcher
!URxvt.clipboard.autocopy: true
!URxvt.keysym.M-c: perl:clipboard:copy
!URxvt.keysym.M-v: perl:clipboard:paste
!URxvt.matcher.button: 1
URxvt.urlLauncher:      firefox
URxvt.tabbed.tabbar-fg: 8
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg:    7
URxvt.tabbed.tab-bg:    0

! XTerminals ------------------------------------------------------------

UXTerm.termName: UXTerm-256color
!UXTerm.vt100..locale: true
UXTerm.vt100..locale: false
UXTerm.vt100..utf8: true
UXTerm.vt100.saveLines: 4096
UXTerm.vt100.scrollBar:          false
UXTerm.vt100.rightScrollBar:     false
UXTerm.vt100.geometry:           96x32
!UXTerm.vt100.background:        #101012
!UXTerm.vt100.foreground:        #E5E5E5
!UXTerm.vt100.cursorColor:       #E5E5E5
!UXTerm.vt100.font:               -xos4-terminus-medium-r-normal-*-16-160-72-72-c-80-iso8859-1
!UXTerm.vt100.boldFont:           -xos4-terminus-medium-r-normal-*-16-160-72-72-c-80-iso8859-1
UXTerm.vt100.faceName:           xft:DejaVu Markup:size=10:antialias=true:autohint=true
!UXTerm.vt100.faceName:          xft:Fira Code:style=regular:size=10:antialias=true:autohint=true

UXTerm.vt100.metaSendsEscape: true
UXTerm.vt100.backarrowKey: false
UXTerm.ttyModes: erase ^?

UXTerm.vt100.translations: #override \n\
    Ctrl <Key>M: maximize() \n\
    Ctrl <Key>R: restore()

UXTerm*VT100.Translations: #override \
     Ctrl Shift <KeyPress> C: copy-selection(CLIPBOARD) \n\
     Ctrl Shift <KeyPress> V: insert-selection(CLIPBOARD)

